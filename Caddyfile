{
admin off
auto_https off
}
:8080 {
log
encode gzip
handle /_upload/* {
    reverse_proxy 127.0.0.1:8000
}
handle /_event/* {
    reverse_proxy 127.0.0.1:8000
}
handle /ping {
    reverse_proxy 127.0.0.1:8000
}
handle {
    root * .web/_static
    try_files {path} {path}/ /index.html
    file_server
}
}
